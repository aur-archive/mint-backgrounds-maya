pkgname=mint-backgrounds-maya
pkgver=1.2
pkgrel=1
pkgdesc="Desktop backgrounds for Linux Mint 13 Maya"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-backgrounds-maya"
license=('unknown')
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('effb47e74458ba872c6dc5c3eb8d3075')

package() {
  cd "${pkgname}"
  cp -R ./usr "${pkgdir}"

}
